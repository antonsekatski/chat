{
  "name": "essential-react",
  "version": "0.5.0",
  "description": "A minimal skeleton for building testable React apps using Babel",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "npm run build",
    "server": "node start.js",
    "build": "webpack -p --progress --config webpack.production.config.js",
    "test": "nyc ava --fail-fast --verbose --require babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pheuter/essential-react.git"
  },
  "keywords": [
    "skeleton",
    "template",
    "react",
    "quickstart"
  ],
  "author": "Mark Fayngersh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pheuter/essential-react/issues"
  },
  "homepage": "https://github.com/pheuter/essential-react",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "ava": "^0.17.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.1"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-runtime": "^6.20.0",
    "beeblebrox": "^1.0.2",
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0",
    "socket-io": "^1.0.0",
    "socket.io": "^1.7.2"
  }
}
